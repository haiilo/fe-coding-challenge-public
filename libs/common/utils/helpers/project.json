{
	"$schema": "../../../../node_modules/nx/schemas/project-schema.json",
	"name": "common-utils-helpers",
	"prefix": "fe-coding-challenge",
	"projectType": "library",
	"sourceRoot": "libs/common/utils/helpers/src",
	"tags": [],
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": [
					"libs/common/utils/helpers/**/*.html",
					"libs/common/utils/helpers/**/*.json",
					"libs/common/utils/helpers/**/*.ts"
				]
			},
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			},
			"executor": "@nrwl/jest:jest",
			"options": {
				"jestConfig": "libs/common/utils/helpers/jest.config.ts",
				"passWithNoTests": true
			},
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
		}
	}
}
